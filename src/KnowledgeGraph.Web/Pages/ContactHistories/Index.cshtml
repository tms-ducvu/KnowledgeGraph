@page
@using KnowledgeGraph.Localization
@using KnowledgeGraph.Web.Pages.ContactHistories
@model IndexModel
@using Microsoft.Extensions.Localization
@inject IStringLocalizer<KnowledgeGraphResource> L

@section scripts {
    <abp-script src="/Pages/ContactHistories/Index.js" />
}

<abp-card>
    <abp-card-header>
        <abp-row>
            <abp-column size-md="_6">
                <abp-card-title>@L["ContactHistories"]</abp-card-title>
            </abp-column>
        </abp-row>
    </abp-card-header>
    <abp-card-body>
        <abp-table striped-rows="true" id="ContactHistoriesTable"></abp-table>
    </abp-card-body>
</abp-card>
